-- SetupConfig: { "AddRequires": [ "Model.CK.GuestUserTable" ] } 

create transformer on CK.vUserAuthProvider
as
begin
	inject "
	  union all
	  select GuestUserId, 'Guest', case LastLoginTime when '0001-01-01' then null else LastLoginTime end
    from CK.tGuestUser
    where GuestUserId > 0
	" after first part {select};
end
